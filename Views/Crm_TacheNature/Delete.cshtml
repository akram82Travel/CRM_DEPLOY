@model CRMSTUBSOFT.Crm_TacheNature

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<div class="content-wrapper">
    <div class="content-header card">
        <div class="content-header-left col-md-6 col-12 breadcrumb-new">
            <h3 class="content-header-title mb-0 d-inline-block">Supprimer Nature</h3>
            <div class="row breadcrumbs-top d-inline-block">
                <div class="breadcrumb-wrapper col-12">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item">
                            <a href="/">Tableau de Bord</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="/Crm_TacheNature">Nature Tache</a>
                        </li>
                        <li class="breadcrumb-item active">
                            Supprimer
                        </li>
                    </ol>
                </div>
            </div>
        </div>

    </div>

    <h3>Are you sure you want to delete this?</h3>
    <div>
        <h4>Nature Tache</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
               Code Nature
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Nature)
            </dd>

            <dt>
                Libelle
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Libelle)
            </dd>

           

        </dl>

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-actions no-color">
                <input type="submit" value="Delete" class="btn btn-danger" />

            </div>
        }
    </div>
</div>
